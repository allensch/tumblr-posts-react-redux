{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "classes": true
        }
    },
    "rules": {
        "object-curly-spacing": ["error", "always"],
        "no-dupe-class-members": [2],
        "no-trailing-spaces": [2],
        "no-dupe-keys": [2],
        "semi": ["error", "never"]
    }
}
